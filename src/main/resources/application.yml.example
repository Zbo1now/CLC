server:
  port: 8081

spring:
  datasource:
    url: jdbc:mysql://localhost:3306/campuscoin?useSSL=false&serverTimezone=UTC&characterEncoding=utf8
    username: root
    password: YOUR_DB_PASSWORD
    driver-class-name: com.mysql.cj.jdbc.Driver
  servlet:
    session:
      timeout: 30m
      cookie:
        http-only: false
        path: /
        same-site: lax

logging:
  level:
    root: INFO

baidu:
  face:
    app-id: "YOUR_APP_ID"
    api-key: "YOUR_API_KEY"
    secret-key: "YOUR_SECRET_KEY"
    group-id: "campus_users"
  bos:
    access-key-id: "YOUR_BOS_ACCESS_KEY"
    secret-access-key: "YOUR_BOS_SECRET_KEY"
    endpoint: "http://bj.bcebos.com"
    bucket-name: "campus-coin-faces"
